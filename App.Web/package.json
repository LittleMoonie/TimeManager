{
  "name": "GoGoTime",
  "version": "1.0.0",
  "private": true,
  "author": "Lazaro, Alexy, Massi, Lounis",
  "description": "Front End for T-DEV-700 Project for Epitech",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 50",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,scss,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/system": "^7.3.3",
    "@reduxjs/toolkit": "^2.3.0",
    "@tabler/icons-react": "^3.21.0",
    "framer-motion": "^11.11.17",
    "react": "^19.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.2.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.28.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/react-redux": "^7.1.34",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^2.1.4",
    "@vitest/ui": "^2.1.4",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "sass": "^1.81.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^2.1.4"
  },
  "packageManager": "yarn@4.10.3",
  "engines": {
    "node": ">=24.9.0",
    "yarn": ">=4.10.3"
  }
}
